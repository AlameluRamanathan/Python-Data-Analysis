import pandas as pd

# Defining a dictionay with website visitors, Bounce_Rate is number of people who visited and left the website immediately
XYZ_web ={"Days":[1,2,3,4,5,6], "Visitors":[1000,700,6000,1000,400,350],"Bounce_Rate":[20,20,23,15,10,34]}

# creating dataframe
df =pd.DataFrame(XYZ_web)
print(df)
